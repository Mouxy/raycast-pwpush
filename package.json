{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "pwpush2",
  "title": "PwPush2",
  "description": "Push Passwords Securely Using PwPush",
  "icon": "extension-icon.png",
  "author": "mouxy",
  "categories": ["Web", "Other"],
  "license": "MIT",
  "commands": [
    {
      "name": "pwpush",
      "title": "Create a Password Push",
      "description": "Create a Password Push Using PwPush.",
      "mode": "view"
    }
  ],
"preferences": [
  {
    "name": "serverUrl",
    "type": "textfield",
    "required": false,
    "title": "Server URL",
    "description": "The URL of the PwPush server.",
    "placeholder": "https://pwpush.com"
  },
  {
    "name": "apiKey",
    "type": "password",
    "required": false,
    "title": "API Key",
    "description": "Your PwPush API key.",
    "placeholder": "Enter your API key"
  }
],
  "dependencies": {
    "@raycast/api": "^1.83.2",
    "node-fetch": "^3.3.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}